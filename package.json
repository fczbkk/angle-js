{
  "name": "@fczbkk/angle",
  "version": "1.0.0",
  "description": "Simple JavaScript library for working with angles.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fczbkk/angle.git"
  },
  "author": {
    "name": "Riki Fridrich",
    "email": "riki@fczbkk.com",
    "url": "http://fczbkk.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fczbkk/angle/issues"
  },
  "homepage": "https://github.com/fczbkk/angle#readme",
  "scripts": {
    "cleanup": "rimraf temp lib",
    "prebuild": "npm run test && npm run cleanup && npm run eslint:build",
    "build": "babel src -d lib",
    "test": "karma start ./karma.conf.js",
    "dev": "npm run test -- --no-single-run --auto-watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postversion": "git push && git push --tags",
    "version": "npm run build && npm run changelog && git add -A",
    "doc": "documentation readme ./src/index.js -s 'Documentation'",
    "eslint": "eslint ./{src,test}/**/*.js",
    "eslint:build": "npm run eslint -- --max-warnings 0"
  },
  "private": true,
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "conventional-changelog-cli": "^1.2.0",
    "documentation": "^4.0.0-beta4",
    "eslint": "^2.11.1",
    "eslint-loader": "^1.3.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1"
  }
}
